<body style="background-color: #b34e3c;">
	<div class="theme-layout">
		<div class="container-fluid pdng0">
			<div class="row merged">
				<div class="offset-md-3 col-md-6">
					<div class="login-reg-bg">
						<div class="log-reg-area sign">
							<div style="text-align: left;"><img src="/img/myNetwork-logo.png" style="width: 100px;"></div><br>

							<h2 class="log-title">Login</h2>

							<form action="/login" method="post">

								<div class="form-group">
									<input type="email" required name="email" />
									<label class="control-label">Email</label><i class="mtrl-select"></i>
								</div>

								<div class="form-group">
									<input type="password" required name="password" />
									<label class="control-label">Password</label><i class="mtrl-select"></i>
								</div>

								<a href="/signup"><p style="color: #b34e3c; "><b>Create an account</b></p></a>
								<div class="submit-btns">
									<button class="mtr-btn login" name="submit" type="submit"><span>Login</span></button>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>


</body>

</html> 